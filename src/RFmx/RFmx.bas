' RFmx Factory Methods
Public Function RFmx_CreateSession( _
        resourceName As String, _
        Optional optionString As String = "" _
    ) As RFmx_Session
    
    Dim isNew As RFmx_Binary
    Dim handle As RFmx_Session
    
    Set handle = New RFmx_Session
    handle.InitSession resourceName, optionString, isNew
    
    Set RFmx_CreateSession = handle
End Function

Public Function RFmx_CreateSessionFromNIRFSASession(rfsaSession As niRFSA_Session) As RFmx_Session
    Dim handle As RFmx_Session
    
    Set handle = New RFmx_Session
    handle.InitSessionFromNIRFSASession rfsaSession
    
    Set RFmx_CreateSessionFromNIRFSASession = handle
End Function

